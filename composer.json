{
  "name": "glhd/conveyor",
  "description": "",
  "keywords": [
    "laravel"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "license": "MIT",
  "require": {
    "illuminate/support": "^8.81",
    "ext-json": "*",
    "mockery/mockery": "^1.3",
    "symfony/console": "^5.4",
    "illuminate/console": "^8.81",
    "jdorn/sql-formatter": "^1.2",
    "illuminate/collections": "^8.81"
  },
  "require-dev": {
    "orchestra/testbench": "^6.21|^7.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.3.2",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Glhd\\Conveyor\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Glhd\\Conveyor\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "fix-style": "vendor/bin/php-cs-fixer fix",
    "check-style": "vendor/bin/php-cs-fixer fix --diff --dry-run"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Glhd\\Conveyor\\Support\\PackageServiceProvider"
      ],
      "aliases": {
        "Gretel": "Glhd\\Conveyor\\Support\\Facades\\Package"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
